{
  "name": "prs-medium-fe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.3.1",
    "@material-ui/icons": "^4.2.1",
    "classnames": "^2.2.6",
    "front-matter": "^3.0.2",
    "lodash.debounce": "^4.0.8",
    "marked": "^0.7.0",
    "mobx": "^5.13.0",
    "mobx-react-lite": "^1.4.1",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "otp-input-react": "^0.2.0",
    "query-string": "^6.8.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-number-format": "^4.3.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-viewer": "^2.10.2",
    "remove-markdown": "^0.3.0",
    "rss-parser": "^3.7.2",
    "socket.io-client": "^2.3.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "watch:css": "tailwind build src/styles/index.css -o src/styles/tailwind.css -w",
    "start:react": "PORT=4008 react-scripts start",
    "build:react": "react-scripts build && cp ./build/index.html ./build/404.html",
    "test": "react-scripts build && cp ./build/index.html ./build/404.html",
    "server": "http-server ./build -p 4008",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sentry/browser": "^5.6.2",
    "@types/classnames": "^2.2.9",
    "@types/lodash.debounce": "^4.0.6",
    "@types/marked": "^0.6.5",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/remove-markdown": "^0.1.1",
    "@types/socket.io-client": "^1.4.32",
    "autoprefixer": "^9.6.5",
    "http-server": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.1.3",
    "typescript": "^3.4.5"
  }
}
