{
  "name": "server",
  "version": "2.0.0",
  "scripts": {
    "db-migrate": "./node_modules/.bin/sequelize db:migrate --debug && yarn wallet-db-migrate",
    "wallet-db-migrate": "./node_modules/.bin/sequelize --options-path ./wallet/.sequelizerc db:migrate --debug",
    "start": "yarn db-migrate && ./node_modules/.bin/nodemon bin/run",
    "start-queue": "./node_modules/.bin/nodemon ./queue.js",
    "start-prod": "export NODE_ENV='production' && yarn db-migrate && ./node_modules/.bin/pm2-runtime bin/run",
    "start-queue-prod": "export NODE_ENV='production' && ./node_modules/.bin/pm2-runtime ./queue.js"
  },
  "nodemonConfig": {
    "ignore": [
      "build",
      "session.json"
    ]
  },
